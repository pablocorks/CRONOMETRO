{
  "manifest_version": 3,
  "name": "Cronômetro Flutuante",
  "version": "1.1",
  "description": "Extensão para um cronômetro flutuante no Chrome.",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}
